<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <% if @title %>
      <title>Arquivo â€“ <%= @title %></title>
    <% else %>
      <title>Arquivo</title>
    <% end %>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <link rel="icon" href="/favicon.svg">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload', 'defer': 'defer' %>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <% if @current_nwo %>
      <meta id="current_nwo" name="<%= @current_nwo %>">
    <% end %>
    <meta id="current_action" name="<%=current_action%>">
  </head>
  <body>
    <div class="container-xl clearfix pt-1">
      <%= yield %>
    </div>
  </body>
</html>
